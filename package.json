{
  "name": "obsidian-tele-go",
  "version": "0.1.0",
  "description": "Obsidian сервер для синхронизации с ТГ ботом через RabbitMQ",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc --noEmit && node esbuild.config.mjs production",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,json,md}\"",
    "fix": "npm run lint:fix && npm run format"
  },
  "keywords": [
    "obsidian",
    "plugin"
  ],
  "author": "markopai",
  "license": "MIT",
  "dependencies": {
    "amqplib": "^0.10.9"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.27.3",
    "obsidian": "^1.12.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
